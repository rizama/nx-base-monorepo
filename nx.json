{
    "npmScope": "nx-base-monorepo",
    "affected": {
        "defaultBase": "master"
    },
    "implicitDependencies": {
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "targetDependencies": {
        "build": [
            {
                "target": "build",
                "projects": "dependencies"
            }
        ]
    },
    "projects": {
        "auth": {
            "tags": []
        },
        "case-kafka": {
            "tags": []
        },
        "data-type": {
            "tags": []
        },
        "nest-mongo": {
            "tags": []
        },
        "nest-typeorm": {
            "tags": []
        },
        "payload-message": {
            "tags": []
        },
        "todos": {
            "tags": []
        }
    }
}
